import javax.swing.JOptionPane;

public class Exercicio01 {
	
	public static void main(String[] args){
		
		int numero = Integer.parseInt(JOptionPane.showInputDialog("Digite um n√∫mero: "));
		
		for(int i = numero; i <=numero; i++  ){
		for(int j = 0; j <=10; j++){
			System.out.println(
					i +"x" +j +"=" +(i*j));
		}
		}
		
	}

}

